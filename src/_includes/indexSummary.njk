<section class="section" id="indexSummary" >
    <nav class="card_quote">
        <header class="card_quote_header">
            <!-- card_quote_icon -->
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="#000000" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="card_quote_icon"><path d="M15 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7Z"/><path d="M14 2v4a2 2 0 0 0 2 2h4"/><path d="M10 9H8"/><path d="M16 13H8"/><path d="M16 17H8"/></svg>
            <h2 class="title2">{{ title }}</h2>
        </header>
        <p class="paragraph_subtitle">
            {{ summary.subtitle }}
        </p>
        <ul class="card_quote_list">
            {% for item in summary.links %}
            <li class="link">
                <a href="{{ item.href }}" class="links">{{ item.text }}</a>
            </li>
            {% endfor %}
        </ul>
    </nav>
</section>
